        (function ($) {
            $.ajax({
                dataType: "json",
                url: "/Resource/GetCulture",
                success: function (data) {
                    if (data && data.length > 0) {
                        languages = $.grep(data, function (n, i) {
                            return n.IsActive == true;
                        });
                    }

                    $.getJSON("/Resource/GetResources", function (data) {
                        resources = data;
