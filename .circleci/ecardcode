        function fnChangeLanguage(obj) {

            var cb = function (data) {
                window.location.href = window.location.href;
            };

            setCulture(obj.value, cb);
        }

        function NavigateToInstNw(e) {
            e.preventDefault();
            (function ($) {
                $.get("/Base/RedirectToInstNetwork",
                    function(data) {
                        if (data === "True") {
                            window.location.href = $(e.target).data("url");
                        }
                    });
            })(jQuery);
        }

        var LoadCulture = function () {
            if (languages && languages.length > 1) {
                $(".ddlLanguage").toggleClass("hide", false);
                var subItems = [];
                subItems.push($("<option/>", { value: "", text: window.resources["SelectLanguage"] }));
                $.each(languages,
                    function (index, item) {
                        subItems.push($("<option/>", { value: item.Code, text: item.NativeLabel }));
                    });
                $(".ddlLanguage").empty().append(subItems);
            }

            var selectedLanguage = GetCookie("_eCardsculture");
            if (selectedLanguage == null) {
                var cb = function (data) { };
                setCulture("en-US", cb);
            }

            $(".ddlLanguage").val(selectedLanguage);
        };

        var setCulture = function (input, cb) {
            $.ajax({
                url: "/Resource/ModifyCulture",
                data: { cultureName: input },
                method: "POST",
                success: function (data) {
                    cb(data);
                },
                cache: false
            });
        };

        function TimeOut(){
            $('#timeOutModal').modal('show');
            TimerLogout();
        }

        function TimerLogout(){
            objLogout = setInterval(Logout, 300000);
        }

        function Logout(){
              $("#lnkLogout")[0].click();
        }
        function ExtendTimeout(){
            clearTimeout(objLogout);
            $.get("/ExtendSession/Extend",
            function(data) {
                if (data === "True") {

                }
            });
        }

        function GetCookie(name) {
            var match = document.cookie.match(new RegExp(name + '=([^;]+)'));
            if (match)
                return match[1];
        }

        $(window).on('load',
            function() {
                if (isOverlayed === "0") {
                    $('#videoOverlayModal').modal('show');
                    var videoSrcauto =
                    "https://player.vimeo.com/video/237659363?modestbranding=1&loop=0&controls=0&showinfo=0&html5=1&autoplay=0";
                    $('#videoOverlayModal iframe').attr('src', videoSrcauto);

                    $('#videoOverlayModal button.close').click(function() {
                        $.get("/Resource/RefreshOverlay",
                            function(data) {
                                if (data === "True") {
                                    $('#videoOverlayModal iframe').attr('src', '');
                                }
                            });
                    });
                }
            });

        $('#videoOverlayModal').on('hidden.bs.modal', function (e) {
            $('#videoOverlayModal').find('iframe').attr('src', '');
        });

